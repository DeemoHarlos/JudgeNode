#include <stdio.h>
int main(){
	int y,j,n,m,d,i,day;
	scanf("%d%d%d",&y,&j,&n);
	for(i=0;i<n;i++){
		scanf("%d%d",&m,&d);
		if(m>12||m<1)
			printf("-1\n");
		else if((m==1||m==3||m==5||m==7||m==8||m==10||m==12)&&(d>31||d<0))
            printf("-2\n");
        else if((m==4||m==6||m==9||m==11)&&(d>30||d<0))
            printf("-2\n");
        else if((m==2&&((y%4==0&&y%100!=0)||y%400==0))&&(d>29||d<0))
            printf("-2\n");
        else if((m==2&&(y%4!=0||(y%100==0&&y%400!=0)))&&(d>28||d<0))
            printf("-2\n");
        else {
        	if((y%4==0&&y%100!=0)||y%400==0){
        		if(m>2&&m<8)
        			day=((m-1)*30+m/2+d+j-2)%7;
        		else
        			day=((m-1)*30+m/2+d+j-1)%7;
        	}
        	else{
        		if(m<=2)
        			day=((m-1)*30+m/2+d+j-1)%7;
        		else if(m>2&&m<8)
        			day=((m-1)*30+m/2+d+j-3)%7;
        		else
        			day=((m-1)*30+m/2+d+j-2)%7;
        	}
        printf("%d\n",day);          
        }
	}
return 0;
}
