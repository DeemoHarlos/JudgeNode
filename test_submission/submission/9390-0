#include <stdio.h>

int main(){
	int n, city[105][105]={0}, a=0, b=0, c=0, d=0;
	scanf("%d", &n);
	for(int i = 1; i <= n; i++){
		for(int j = 1; j <= n; j++){
			scanf("%d", &city[i][j]);
		}
	}
	for(int i = 1; i <= n; i++){
		for(int j = 1; j <= n; j++){
			int count = 0;
			if(city[i][j]==1){
				if(city[i-1][j]==1){
					count++;
				}
				if(city[i+1][j]==1){
					count++;
				}
				if(city[i][j-1]==1){
					count++;
				}
				if(city[i][j+1]==1){
					count++;
				}
				if(count==2 && ((city[i-1][j]==1 && city[i+1][j]==1) || (city[i][j-1]==1 && city[i][j+1]==1))){
					continue;
				}
				switch(count){
					case 1:
						d++;
						break;
					case 2:
						c++;
						break;
					case 3:
						b++;
						break;
					case 4:
						a++;
						break;
				}
			}
		}
	}
	printf("%d\n%d\n%d\n%d\n", a, b, c, d);
	return 0;
}